{
  "type": "component",

  "props": {
    "value": {
      "type": [ "Object" ],
      "desc": "Model of the component; Either use this property (along with a listener for 'input' event) OR use v-model directive",
      "required": true,
      "examples": [ "v-model=\"myText\"" ],
      "category": "model"
    },

    "max-years": {
      "type": [ "Number" ],
      "desc": "",
      "default": 2,
      "category": "model"
    },

    "locale": {
      "type": [ "String" ],
      "desc": "The language of the component",
      "category": "style"
    },
    "no-compare": {
      "type": [ "Boolean" ],
      "desc": "Disable compare mode",
      "category": "style"
    },

    "color": {
      "type": [ "String" ],
      "desc": "Color name for component from the Quasar Color Palette",
      "examples": [ "primary", "teal-10" ],
      "default": "primary",
      "category": "style"
    },

    "previous-color": {
      "type": [ "String" ],
      "desc": "Color name for component from the Quasar Color Palette for the previous period",
      "examples": [ "amber", "cyan-6" ],
      "default": "amber",
      "category": "style"
    },

    "toggle-color": {
      "type": [ "String" ],
      "desc": "Color name for component from the Quasar Color Palette for the compare toggle",
      "examples": [ "secondary", "blue" ],
      "default": "secondary",
      "category": "style"
    },

    "transition-show": {
      "desc": "One of Quasar's embedded transitions",
      "default": "jump-left",
      "examples": [ "fade", "slide-down" ],
      "category": "behavior"
    },

    "transition-hide": {
      "desc": "One of Quasar's embedded transitions",
      "default": "jump-right",
      "examples": [ "fade", "slide-down" ],
      "category": "behavior"
    }
  },
  "events": {
    "input": {
      "extends": "input",
      "params": {
        "value": {
          "type": [ "Object" ]
        }
      }
    }
  }
}
