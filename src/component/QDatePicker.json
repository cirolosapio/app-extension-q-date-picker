{
  "type": "component",
  "props": {
    "value": {
      "type": [
        "Object"
      ],
      "desc": "Model of the component; Either use this property (along with a listener for 'input' event) OR use v-model directive",
      "required": true,
      "examples": [
        "v-model=\"myText\""
      ],
      "category": "model"
    },
    "now": {
      "type": [
        "String"
      ],
      "desc": "Reference day",
      "examples": [
        "2020-06-01"
      ],
      "category": "model"
    },
    "compare": {
      "type": [
        "Boolean"
      ],
      "desc": "Enable compare mode",
      "category": "behavior"
    },
    "hideCompareToggle": {
      "type": [
        "Boolean"
      ],
      "desc": "Hide the compare toggle",
      "category": "behavior"
    },
    "clearable": {
      "type": [
        "Boolean"
      ],
      "desc": "Enable clearable button",
      "category": "behavior"
    },
    "displayFormat": {
      "type": [
        "String"
      ],
      "desc": "Display format for the selected range",
      "examples": [
        "D MMMM YYYY",
        "D MMMM YY",
        "D MMM YY"
      ],
      "default": "D MMM YYYY",
      "category": "style"
    },
    "color": {
      "type": [
        "String"
      ],
      "desc": "Color name for component",
      "examples": [
        "blue",
        "purple",
        "red",
        "green",
        "teal",
        "orange",
        "indigo",
        "yellow",
        "pink"
      ],
      "default": "blue",
      "category": "style"
    },
    "prev-color": {
      "type": [
        "String"
      ],
      "desc": "Color name for component",
      "examples": [
        "blue",
        "purple",
        "red",
        "green",
        "teal",
        "orange",
        "indigo",
        "yellow",
        "pink"
      ],
      "default": "orange",
      "category": "style"
    },
    "toggle-color": {
      "type": [
        "String"
      ],
      "desc": "Color name for component from the Quasar Color Palette for the compare toggle",
      "examples": [
        "secondary",
        "blue"
      ],
      "default": "secondary",
      "category": "style"
    },
    "menu-props": {
      "type": [
        "Object"
      ],
      "desc": "Props configuration for popup-proxy",
      "default": "{ transitionShow: 'jump-left', transitionHide: 'jump-right', anchor: 'top right' }",
      "category": "style"
    },
    "labels": {
      "type": [
        "Object"
      ],
      "desc": "Label customization",
      "default": "{ custom: 'Custom', last_year: 'Last Year', compare: 'Compare', prev_period: 'Previous Period' }",
      "category": "style"
    }
  },
  "events": {
    "input": {
      "extends": "input",
      "params": {
        "value": {
          "desc": "New model value",
          "type": [
            "Object"
          ],
          "required": true
        }
      }
    }
  }
}